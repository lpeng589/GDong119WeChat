.m{
    margin: auto;
    position: absolute;
    top: 55%;
    left: 40%;
    text-align: center;
}
.img{
    width: 64px;
    height: 64px;
}
canvas{
    width: 100%;
    height: 100%;
    z-index: 999;
}
.tip{
    position: absolute;
    top:20px;
    width: 85%;
    text-align: left;
    left: 5%;
    right: 5%;
    margin: auto;
    background-color: #fff;
    padding: 8px;
    font-size: 14px;
    line-height: 22px;
    border-radius: 3px;
    color:#666666;
}
.showimg{
    padding-top: 5%;
    z-index: 1000;
}
.picfont{
    color: #2DACF3;
    position:absolute;
    top:28%;
    width:100%;
    text-align: center;
}
.mask{
    opacity: 1.0;
}